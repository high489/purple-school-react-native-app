{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "ignoreDeprecations": "6.0",
    "strict": true,
    "paths": {
      "@app": ["./app/"],
      "@app/*": ["./app/*"],
      "@assets": ["./assets/"],
      "@assets/*": ["./assets/*"],
      "@shared": ["./shared/"],
      "@shared/*": ["./shared/*"]
    }
  }
}
